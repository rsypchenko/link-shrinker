// Prisma schema for URL shortener
// Save this as schema.prisma in the prisma directory

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Url {
  id    Int    @id @default(autoincrement())
  code  String @unique
  url   String
}
